def sort_check(l):
    x=l[0]
    dim=len(x)
    status=[]
    for s in range(dim):
        status.append(x[s])
    for elem in l:
        test=True
        for s in range(-1,-dim-1,-1):
            if elem[s]<status[s] and test:
                test=False
            if elem[s]>status[s] and (not test):
                test=True
        if (not test):
            print("Fail")
            return -1
        status=elem
        return 0


